{
  "compilerOptions": {
    "sourceMap": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "target": "es2015",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": ["es6"],
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "./node_modules"
  ]
}
